{
  "name": "athena-client",
  "version": "1.0.0",
  "description": "ATHENA Platform Frontend",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:web": "next dev -p 3000",
    "dev:api": "npm --prefix ../server run dev",
    "dev:e2e": "concurrently -k -s first \"npm run dev:web\" \"npm run dev:api\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:i18n": "node scripts/i18n-lint.js",
    "test": "start-server-and-test \"npm run dev:e2e\" http://localhost:3000 \"playwright test\"",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "e2e": "playwright test"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.1.1",
    "@hookform/resolvers": "^3.3.3",
    "@tanstack/react-query": "^5.17.0",
    "axios": "^1.6.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.2.0",
    "framer-motion": "^10.17.9",
    "i18next": "^23.16.8",
    "lucide-react": "^0.306.0",
    "next": "^14.0.4",
    "next-auth": "^4.24.5",
    "next-i18next": "^15.4.0",
    "posthog-js": "^1.331.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^14.1.3",
    "react-media-recorder": "^1.7.2",
    "socket.io-client": "^4.8.3",
    "tailwind-merge": "^2.2.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.33",
    "start-server-and-test": "^2.0.13",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
